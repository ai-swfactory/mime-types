{
  "name": "@ai-swfactory/mime-types",
  "version": "1.0.0",
  "description": "TypeScript MIME type database and utilities with full type inference",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-swfactory/mime-types.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "panoptic-build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test"
  },
  "devDependencies": {
    "@ai-swfactory/panoptic-presets": "^12.0.0",
    "@ai-swfactory/rollup-plugins": "^2.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint": "^9.18.0",
    "jiti": "^2.4.2",
    "prettier": "^3.4.2",
    "semantic-release": "^25.0.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
}
